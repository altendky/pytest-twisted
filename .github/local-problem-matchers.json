{
    "problemMatcher": [
        {
            "owner": "local-generic-warning",
            "severity": "warning",
            "pattern": [
                {
                    "regexp": "^(.*\\bWARNING:\\b.*)$",
                    "message": 1
                }
            ]
        },
        {
            "owner": "local-tox-not_in_env_warning",
            "severity": "error",
            "pattern": [
                {
                    "regexp": "^(\\s*WARNING: test command found but not installed.*)$",
                    "message": 1
                },
                {
                    "regexp": "^(\\s*(cmd|env): .*)$",
                    "message": 1,
                    "loop": true
                },
                {
                    "regexp": "^(\\s*Maybe you forgot.*)$",
                    "message": 1
                }
            ]
        }
    ]
}
